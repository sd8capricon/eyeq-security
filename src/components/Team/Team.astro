---
import TeamCard from "./TeamCard.astro";

import Member1 from "../../assets/img/team-1.jpg";
import Member2 from "../../assets/img/team-2.jpg";
import Member3 from "../../assets/img/team-3.jpg";
---

<div class="container py-5">
    <div class="row g-5">
        <div class="col-lg-4 wow slideInUp" data-wow-delay="0.3s">
            <TeamCard src={Member1} />
        </div>
        <div class="col-lg-4 wow slideInUp" data-wow-delay="0.6s">
            <TeamCard src={Member2} />
        </div>
        <div class="col-lg-4 wow slideInUp" data-wow-delay="0.9s">
            <TeamCard src={Member3} />
        </div>
    </div>
</div>
